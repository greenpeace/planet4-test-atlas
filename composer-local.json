{
	"name": "greenpeace/planet4-test-atlas",
	"description": "Greenpeace P4 test-atlas ",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-master-theme": "7",
		"greenpeace/planet4-plugin-gutenberg-blocks": "dev-main"
	},
	"scripts": {
		"install:plugin-wpimport": "wp plugin install --activate https://storage.googleapis.com/planet4-3rdparty-plugins/wp-all-import-pro_4.6.3.zip",
		"core:activate": "wp theme activate planet4-master-theme",
		"site:custom": [
			"@install:plugin-wpimport",
			"@core:activate"
		]
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "greenpeace/planet4-master-theme",
				"type": "wordpress-theme",
				"autoload": {
					"psr-4": {
						"P4\\MasterTheme\\": "src/"
					}
				},
				"extra": {
					"installer-paths": {
						"vendor/plugins/{$name}/": [
							"type:wordpress-plugin"
						]
					}
				},
				"version": 7,
				"dist": {
					"type": "zip",
					"url": "https://storage.googleapis.com/planet4-packages/planet4-master-theme/8bcb1a0056b31e3148be04ee017a4320ef1afebd.zip"
				}
			}
		}
	]
}
