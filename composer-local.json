					"psr-4": {
						"P4\\MasterTheme\\": "src/"
					}
				},
				"extra": null,
				"version": 7,
				"dist": {
					"type": "zip",
					"url": "https://storage.googleapis.com/planet4-packages/planet4-master-theme/44ac7703fa11f8c826f7e9629c3f141a0f8ec1d7.zip"
				}
			}
		}
	]
}
