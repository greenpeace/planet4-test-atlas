{
	"name": "greenpeace/planet4-test-atlas",
	"description": "Greenpeace P4 test-atlas ",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-master-theme": "7"
	},
	"scripts": {
		"core:language": "wp language core install el; wp language core install hu_HU; wp language core install nl_NL; wp language core install pt_BR; wp language core install ar;",
